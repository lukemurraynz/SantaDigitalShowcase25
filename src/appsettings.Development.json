{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Limits": {
    "TopN": 3,
    "DebounceSeconds": 5,
    "Timeouts": {
      "DefaultMs": 15000
    }
  }
  ,"Diagnostics": {
    "EnableRequestLogging": true
  },
  "Cosmos": {
    "Endpoint": "https://YOUR-COSMOS.documents.azure.com:443/",
    "Key": "__PLACEHOLDER__",
    "DatabaseName": "elves_demo",
    "Containers": {
      "Children": "children",
      "Wishlists": "wishlists",
      "Recommendations": "recommendations",
      "Profiles": "profiles",
      "Assessments": "assessments",
      "Notifications": "notifications",
      "Leases": "leases"
    }
  }
}
